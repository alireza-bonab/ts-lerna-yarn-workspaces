{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    /* Module Resolution Options */
    "moduleResolution": "node",
    "esModuleInterop": true,
    /* Advanced Options */
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    /* Basic Options */
    "declaration": true,
    "declarationMap": true,
    "composite": true,
   
  }
}
